	</screenshots>
	<changelog>
		<change>Added some 3rd party icons.</change>
	</changelog>
	<links>
		<link>
			<name>Github</name>
			<url>https://github.com/moonstar-x</url>
			<iconclass>fa fa-github</iconclass>
		</link>
		<link>
